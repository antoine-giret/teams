{
  "expo": {
    "name": "Teams",
    "slug": "teams",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "entryPoint": "./index.ts",
    "icon": "./assets/image/icon.png",
    "splash": {
      "image": "./assets/image/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3f51b5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "androidStatusBar": {
      "backgroundColor": "#3f51b5"
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
